{
  "name": "@ibieats/db",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "migrations:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migrations",
    "migrations:run": "ts-node -r tsconfig-paths/register src/run-migrations.ts",
    "seed:run": "ts-node -r tsconfig-paths/register src/seed.ts",
    "seed:run:prod": "node dist/libs/db/seed.js",
    "migrations:run:prod": "node dist/libs/db/run-migrations.js"
  },
  "dependencies": {
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/node": "^22.10.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typeorm": "^0.3.17"
  },
  "license": "MIT"
}
